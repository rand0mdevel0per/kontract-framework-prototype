    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.3",
    "@babel/types": "^7.26.3",
    "@vitest/coverage-v8": "^1.6.1",
    "@types/babel__traverse": "^7.20.5",
    "c8": "^9.1.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.0",
    "vitepress": "^1.0.4",
    "vitest": "^1.5.0"
  }
}
